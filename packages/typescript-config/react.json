{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "useDefineForClassFields": true,
        "jsx": "react-jsx",
        "noEmit": true,
        "moduleDetection": "force",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true
    }
}